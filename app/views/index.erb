<% if current_user %>
  <div class="new-finstagram-post">
    <a href="/finstagram_posts/new" class="button">New Post</a>
  </div>
<% end %>


<% @finstagram_posts.each do |finstagram_post| %>

  <!-- use the partial -->
  <%= erb(:'shared/finstagram_post', { locals: { f_post: finstagram_post } }) %>

<% end %>